							+tr-line("Справка для надомного обучения","2000","dark-line")
							+tr-line("Справка от педиатра","1500")
							+tr-line("Освобождение от физкультуры","1500","dark-line")
				+e.order-information
					+e.footer-block
						+e.order-text Для заказа любой справки звоните по телефону с 09:00 до 22:00 по МСК:
						+e.A.phone-block(href="tel:+7(495)108-74-55")
							+e.phone-first +7 (495) 
							+e.phone-second 108-74-55

	// end s-price